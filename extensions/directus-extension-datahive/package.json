{
  "name": "directus-extension-datahive",
  "version": "1.0.0",
  "type": "module",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "directus:extension": {
    "host": "^10.1.14",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "directus-datahive-endpoints",
        "source": "src/directus-datahive-endpoints/index.ts"
      },
      {
        "type": "hook",
        "name": "directus-datahive-hooks",
        "source": "src/directus-datahive-hooks/index.ts"
      },
      {
        "type": "module",
        "name": "directus-datahive-modules",
        "source": "src/directus-datahive-modules/index.ts"
      }
    ]
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add",
    "test": "echo \"Error: oops, there is no tests yet !\" && exit 1",
    "start": "node ./dist/databee/process.js $PROJECT_ID",
    "resume": "CRAWLEE_PURGE_ON_START=0 node ./src/directus-datahive-databee/src/main.js --resume $RUN_ID"
  },
  "dependencies": {
    "async-mutex": "^0.4.0",
    "crawlee": "^3.5.8",
    "js-md5": "^0.8.3",
    "nodemailer": "^6.9.7",
    "p-limit": "^5.0.0",
    "playwright": "*",
    "ps-node": "^0.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.14",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.14",
    "@types/ps-node": "^0.1.3",
    "dotenv": "^16.3.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-externals": "6.1.2",
    "typescript": "^5.2.2",
    "vue": "^3.3.7"
  }
}
