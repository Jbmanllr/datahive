{
	"name": "directus-extension-datahive",
	"version": "1.0.0",
	"type": "module",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"directus:extension": {
		"host": "^10.1.14",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "directus-datahive-endpoints",
				"source": "src/directus-datahive-endpoints/index.ts"
			},
			{
				"type": "module",
				"name": "directus-datahive-pollinator",
				"source": "src/directus-datahive-pollinator/index.ts"
			},
			{
				"type": "module",
				"name": "directus-datahive-metamorph",
				"source": "src/directus-datahive-metamorph/index.ts"
			},
			{
				"type": "module",
				"name": "directus-datahive-databee",
				"source": "src/directus-datahive-databee/index.ts"
			}
		]
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add",
		"start": "node ./src/directus-datahive-databee/src/main.js 07f791aa-bf0c-48ae-8732-7e6b3cb18ff8",
		"resume": "CRAWLEE_PURGE_ON_START=0 node ./src/directus-datahive-databee/src/main.js --resume f90726b2-a52c-400d-97d6-eddedc2d168",
		"test": "echo \"Error: oops, there is no tests yet, sad ;(!\" && exit 1",
		"postinstall": "npx crawlee install-playwright-browsers"
	},
	"dependencies": {
		"axios": "^1.6.0",
		"dotenv": "^16.3.1",
		"crawlee": "^3.5.8",
		"playwright": "*",
		"winston": "^3.11.0",
		"p-limit": "^4.0.0"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.14",
		"@types/node": "^20.8.10",
		"typescript": "^5.2.2",
		"vue": "^3.3.7"
	}
}