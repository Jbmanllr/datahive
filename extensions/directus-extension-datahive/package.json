{
	"name": "directus-extension-datahive",
	"version": "1.0.0",
	"type": "module",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"directus:extension": {
		"host": "^10.1.14",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "directus-datahive-endpoints",
				"source": "src/directus-datahive-endpoints/index.ts"
			},
			{
				"type": "hook",
				"name": "directus-datahive-hooks",
				"source": "src/directus-datahive-hooks/index.ts"
			},
			{
				"type": "module",
				"name": "directus-datahive-modules",
				"source": "src/directus-datahive-modules/index.ts"
			}
		]
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add",
		"start": "node ./src/datahive-core/databee/main.ts 07f791aa-bf0c-48ae-8732-7e6b3cb18ff8",
		"resume": "CRAWLEE_PURGE_ON_START=0 node ./src/directus-datahive-databee/src/main.js --resume f90726b2-a52c-400d-97d6-eddedc2d168",
		"test": "echo \"Error: oops, there is no tests yet, sad ;(!\" && exit 1"

	},
	"dependencies": {
		"rollup-plugin-node-externals": "^6.1.1",
		"datahive-core": "file:../../datahive-core"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.14",
		"@types/node": "^20.8.10",
		"typescript": "^5.2.2",
		"vue": "^3.3.7"
	}
}
